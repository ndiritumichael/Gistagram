CREATE TABLE TokenEntity(
accessToken TEXT DEFAULT NULL PRIMARY KEY ,
scope TEXT DEFAULT NULL,
tokenType TEXT DEFAULT NULL
);

-- Gets access token field from Token Table
getToken:
SELECT ALL accessToken
FROM TokenEntity
WHERE accessToken IS NOT NULL;

-- Create Token Table and saves token value returned from API response
saveToken:
INSERT OR REPLACE
INTO TokenEntity
VALUES (?,?,?);

-- Clears Token table values
deleteToken:
DELETE FROM TokenEntity;